{
  "version": 2,
  "builds": [
    { "src": "/api/get-market-data", "use": "@vercel/node" },
    { "src": "/api/post-day-results", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/get-market-data",
      "methods": ["GET"],
      "dest": "/api/get-market-data.js"
    },
    {
      "src": "/api/post-day-results",
      "methods": ["POST"],
      "dest": "/api/post-day-results.js"
    }
  ]
}
